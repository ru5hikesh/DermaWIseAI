# Django backend specific .gitignore

# Python bytecode
__pycache__/
*.py[cod]
*$py.class

# Distribution / packaging
*.egg
*.egg-info/
dist/
build/
eggs/
parts/
bin/
var/
sdist/
develop-eggs/
.installed.cfg

# Virtual environments
.venv/
env/
ENV/
venv/

# Django stuff
db.sqlite3
db.sqlite3-journal
*.log
local_settings.py

# Environment variables
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Django static/media
/media/
/static/
staticfiles/
collected_static/

# Django migrations (optional, uncomment if needed)
# */migrations/*.py
# !*/migrations/__init__.py

# Django test coverage
.coverage
htmlcov/
.tox/
.coverage.*
.cache/
nosetests.xml
coverage.xml
*.cover

# IDE specific files
.idea/
.vscode/
*.swp
*.swo

# macOS specific files
.DS_Store
.AppleDouble
.LSOverride

# ML model files and datasets
*.h5
*.pt
*.ckpt
*.pkl
*.npz
*.joblib
*.tflite
*.onnx

# Datasets
predictor/dataset/
!predictor/dataset/HAM10000_metadata*  # keep metadata CSV if needed

# Temporary files
*.bak
*.tmp
*~

# Celery stuff (if using)
celerybeat-schedule
celerybeat.pid

# Jupyter Notebook
.ipynb_checkpoints

# Ignore generated pip installer
get-pip.py
